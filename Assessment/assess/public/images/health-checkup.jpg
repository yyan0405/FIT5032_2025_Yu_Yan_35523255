<svg xmlns="http://www.w3.org/2000/svg" width="300" height="200" viewBox="0 0 300 200">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#e3f2fd;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#bbdefb;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="300" height="200" fill="url(#bg)"/>
  
  <!-- Stethoscope -->
  <circle cx="150" cy="80" r="25" fill="#2196f3" stroke="#1976d2" stroke-width="3"/>
  <circle cx="150" cy="80" r="15" fill="#fff"/>
  <path d="M150 55 Q130 40 110 50 Q100 55 105 65 Q110 70 120 65 Q140 55 150 55" fill="#1976d2"/>
  <path d="M150 55 Q170 40 190 50 Q200 55 195 65 Q190 70 180 65 Q160 55 150 55" fill="#1976d2"/>
  
  <!-- Heart rate line -->
  <path d="M50 120 L80 120 L90 100 L100 140 L110 80 L120 160 L130 120 L250 120" 
        stroke="#f44336" stroke-width="3" fill="none"/>
  
  <!-- Medical cross -->
  <rect x="70" y="140" width="20" height="40" fill="#4caf50"/>
  <rect x="60" y="150" width="40" height="20" fill="#4caf50"/>
  
  <!-- Clipboard -->
  <rect x="200" y="130" width="60" height="50" fill="#fff" stroke="#666" stroke-width="2" rx="3"/>
  <rect x="215" y="125" width="30" height="10" fill="#666" rx="2"/>
  <line x1="210" y1="145" x2="250" y2="145" stroke="#333" stroke-width="1"/>
  <line x1="210" y1="155" x2="240" y2="155" stroke="#333" stroke-width="1"/>
  <line x1="210" y1="165" x2="245" y2="165" stroke="#333" stroke-width="1"/>
  
  <!-- Title -->
  <text x="150" y="25" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#1976d2">Free Health Checkup</text>
</svg>