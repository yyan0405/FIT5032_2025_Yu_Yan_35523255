  <path d="M120 70 Q122 68 124 70" stroke="#4caf50" stroke-width="2" fill="none"/>
  
  <!-- Grapes -->
  <circle cx="110" cy="140" r="4" fill="#9c27b0"/>
  <circle cx="115" cy="145" r="4" fill="#9c27b0"/>
  <circle cx="105" cy="145" r="4" fill="#9c27b0"/>
  
  <!-- Rice/grains -->
  <ellipse cx="130" cy="140" rx="8" ry="5" fill="#ffc107"/>
  <ellipse cx="125" cy="145" rx="6" ry="3" fill="#ffc107"/>
  
  <!-- Nutritionist figure -->
  <circle cx="50" cy="60" r="15" fill="#ffb74d"/>
  <rect x="40" y="75" width="20" height="30" fill="#fff" rx="10"/>
  <rect x="35" y="85" width="10" height="20" fill="#ffb74d"/>
  <rect x="55" y="85" width="10" height="20" fill="#ffb74d"/>
  
  <!-- Clipboard with nutrition plan -->
  <rect x="25" y="120" width="30" height="40" fill="#fff" stroke="#666" stroke-width="2" rx="3"/>
