  <ellipse cx="180" cy="130" rx="15" ry="8" fill="#ff7043"/>
  <path d="M195 130 L205 125 L205 135 Z" fill="#ff7043"/>
  <circle cx="175" cy="128" r="2" fill="#333"/>
  
