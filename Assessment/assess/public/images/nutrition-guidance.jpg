  <line x1="30" y1="130" x2="50" y2="130" stroke="#333" stroke-width="1"/>
  <line x1="30" y1="140" x2="45" y2="140" stroke="#333" stroke-width="1"/>
  <line x1="30" y1="150" x2="50" y2="150" stroke="#333" stroke-width="1"/>
  
  <!-- Nutrition symbols -->
  <text x="250" y="50" font-family="Arial, sans-serif" font-size="20" fill="#4caf50">🥗</text>
  <text x="270" y="100" font-family="Arial, sans-serif" font-size="18" fill="#ff5722">🍎</text>
  <text x="240" y="160" font-family="Arial, sans-serif" font-size="16" fill="#ff9800">🥕</text>
  
