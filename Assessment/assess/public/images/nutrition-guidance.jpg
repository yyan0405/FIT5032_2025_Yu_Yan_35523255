  <!-- Protein section -->
  <path d="M150 110 L210 110 A60 60 0 0 1 150 170 Z" fill="#ff5722" opacity="0.7"/>
  
  <!-- Grains section -->
  <path d="M150 110 L150 170 A60 60 0 0 1 90 110 Z" fill="#ff9800" opacity="0.7"/>
  
  <!-- Fruits section -->
  <path d="M150 110 L90 110 A60 60 0 0 1 150 50 Z" fill="#e91e63" opacity="0.7"/>
  
  <!-- Food items -->
  <!-- Broccoli -->
  <circle cx="180" cy="80" r="8" fill="#2e7d32"/>
  <rect x="178" y="88" width="4" height="10" fill="#4caf50"/>
  
  <!-- Carrot -->
  <path d="M170 95 L175 105 L165 105 Z" fill="#ff5722"/>
  <path d="M170 95 Q168 92 170 90" stroke="#4caf50" stroke-width="2" fill="none"/>
  
